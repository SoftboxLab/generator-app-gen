{
  "name": "generator-app-gen",
  "version": "0.0.9",
  "description": "Code generator based on Yeoman Generators.",
  "preferGlobal": true,
  "contributors": [
    {
      "name": "Tarcísio Abadio de Magalhães Júnior",
      "email": "tarcisiojunior@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SoftboxLab/generator-app-gen.git"
  },
  "main": "index.js",
  "keywords": [
    "yeoman-generator", "yo", "code", "generation", "generate", "template", "scaffold", "scaffolding"
  ],
  "scripts": {
    "test": "mocha --reporter spec --require blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Tarcísio Abadio de Magalhães Júnior",
  "license": "ISC",
  "dependencies": {
    "async": "^0.9.0",
    "chalk": "^1.0.0",
    "lodash": "^3.3.1",
    "merge": "^1.2.0",
    "mysql": "^2.5.5",
    "underscore": "^1.8.2",
    "yeoman-generator": "^0.18.9"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "coveralls": "^2.11.2",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "mockrequire": "0.0.3",
    "proxyquire": "^1.3.1",
    "travis-cov": "^0.2.5"
  },
  "config": {
    "blanket": {
      "pattern": [
        "drivers-in",
        "plugins",
        "generators"
      ],
      "data-cover-never": "node_modules"
    },
    "travis-cov": {
      "threshold": 70
    }
  }
}
